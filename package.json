    "expo-updates": "~29.0.15",
    "expo-video": "~3.0.15",
    "nostr-tools": "^2.19.2",
    "qrcode": "^1.5.4",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "react-native-get-random-values": "~1.11.0",
    "react-native-qrcode-svg": "^6.3.21",
    "react-native-safe-area-context": "^5.6.2",
    "react-native-screens": "~4.16.0",
